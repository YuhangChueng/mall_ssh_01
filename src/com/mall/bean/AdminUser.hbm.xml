<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping package="com.mall.bean">
   <class name="AdminUser" table="Adminuser" catalog="mall">
      <id name="admin_id" column="admin_id">
         <generator class="native"/>
      </id>
      <property name="admin_name" column="admin_name"/>
      <property name="admin_psw" column="admin_psw"/>
      <property name="lastLoginTime" column="lastLoginTime"/>
   </class>
</hibernate-mapping>
